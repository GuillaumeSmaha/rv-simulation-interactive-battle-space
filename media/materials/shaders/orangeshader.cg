// -----------------------------------------------------------
 // Very simple shader
 // The object is colored in orange
 // -----------------------------------------------------------
 
 float4 main_fp(in float3 TexelPos : TEXCOORD0) : COLOR
 {
     float4 oColor;
 
     oColor.r = 1.0;
     oColor.g = 0.8;
     oColor.b = 0.0;
     oColor.a = 0.0;
 
     return oColor;
 }
